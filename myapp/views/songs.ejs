<% include partials/header %>
 <h1>Songs List</h1>
 <hr/>
 <table>
    <tr>
      <th>Album</th>
      <th>Artist</th>
      <th>Title</th>
      <th>Year</th>
      <th>BPM</th>
    </tr>

<% for(var i=0; i<songs.length; i++) {%>
    <tr>
           <td>
             <%= songs[i].album%>
           </td>

           <td>
             <%= songs[i].artist%>
           </td>

           <td>
             <%= songs[i].title%>
           </td>

           <td>
             <%= songs[i].year%>
           </td>
      
           <td>
             <%= songs[i].bpm%>
           </td>
      <% } %>
    </tr>

 </table>
<% include partials/footer %>
